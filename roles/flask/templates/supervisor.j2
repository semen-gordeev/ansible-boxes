[program:{{ app_name }}]
command={{ virtualenv_path }}/bin/uwsgi --ini {{ project_dir }}/uwsgi.ini --virtualenv {{ virtualenv_path }} --chdir {{ project_dir }}
autostart=true
autorestart=true
stderr_logfile={{ log_dir }}/supervisor.err.log
stdout_logfile={{ log_dir }}/supervisor.out.log
redirect_stderr=true
stopwaitsecs=60
stopsignal=INT